charSection:AddSlider({
	Name = "Speed",
	Min = 16,
	Max = 10244,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	Save = true,
	Flag = "speed",
	ValueName = "is the speed value",
	Callback = function(yummyspeed)
		game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = yummyspeed
	end    
})

charSection:AddSlider({
	Name = "Jump Height",
	Min = 50,
	Max = 10244,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Save = true,
	Flag = "jp",
	Increment = 1,
	ValueName = "is the jump height value",
	Callback = function(yummyjp)
		game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = yummyjp
	end    
})

print(OrionLib.Flags["speed"].Value)

print(OrionLib.Flags["jp"].Value)

settingsSection:AddParagraph("Read before doing anything.", "Do not share your uniquely generated ID. It will change every time you execute this script again for security purposes. Staff never ask for this ID.")

local id = game:GetService("HttpService"):GenerateGUID(false)

settingsSection:AddParagraph("Your uniquely generated ID is ", id)

OrionLib:Init()
